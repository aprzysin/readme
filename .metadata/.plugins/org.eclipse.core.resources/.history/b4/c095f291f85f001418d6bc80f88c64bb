import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class Server {
	private Data[] datas;
	public void load() throws FileNotFoundException  {
		File file = new File("src/blackopsOutput.txt");
		this.datas = new Data[this.numLines()];
		Scanner fileReader = new Scanner(new File("src/blackops.txt"));
		int i = 0;
		while (fileReader.hasNext()) {
			// counting the number of lines
			this.datas[i] = new Data(fileReader.nextDouble(), fileReader.nextDouble());
			i++;
		}
	}
	private int numLines() throws FileNotFoundException  {
		Scanner lineReader = new Scanner("src/blackops.txt");
		int lines  = 0;
		while (lineReader.hasNextLine()) {
			lineReader.nextLine();
			lines++;
		}
		return lines;
	}
	private void analyze() throws FileNotFoundException {
		System.out.println("Number of Lines" + this.numLines());
	}
	

}
