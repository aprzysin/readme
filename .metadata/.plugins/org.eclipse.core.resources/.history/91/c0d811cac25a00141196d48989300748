import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class Data {
	// variable to track the players rank
	private double rank;
	// variable to track the players score
	private double score;
	//variable used show the Average score compared to the blackops.txt ranks
	private double averageScore;
	
	public Rank(double startRank = 0, double startScore, double startAverageScore) throws FileNotFoundException  {
		// creates a file input.txt in src and creates a scanner to read this
		// file
		Scanner fileReader = new Scanner(new File("src/blackops.txt"));
		startRank = fileReader.nextDouble();
		startScore = fileReader.nextDouble();
		startScore
	}
}
