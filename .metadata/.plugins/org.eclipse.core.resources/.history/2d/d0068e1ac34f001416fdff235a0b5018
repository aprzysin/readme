import java.io.*;
import java.util.*;

public class Project4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) throws FileNotFoundException {
		Scanner fileReader = new Scanner(new File("src/input.txt"));
		String date = fileReader.next();
		String item = fileReader.next();
		String action = fileReader.next();
		int amount = fileReader.nextInt();
		while (fileReader.hasNextLine()) {
		System.out.println(date + item + action + amount);
		System.out.println(fileReader.next());
		System.out.println(fileReader.next());
		System.out.println(fileReader.nextDouble());
		}
		}

		

		// TODO Auto-generated method stub

	}


