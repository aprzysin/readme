package chpt1_1;

/**
 * @author Andrew
 *
 */
public class chpt11class {

	/**
	 * @param args
	 * @throws InterruptedException 
	 */
	public static void main(String[] args) throws InterruptedException {
		for ( int i = 1; i <= 10; i++) {
			for (int x = 1; x <= i; x++) {
			System.out.print(i);
			System.in.wait(5);
		}
			System.out.println();
	}
		// TODO Auto-generated method stub

	}

}
