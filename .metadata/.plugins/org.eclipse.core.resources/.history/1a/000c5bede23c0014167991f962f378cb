public class chpt3test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.print(":(");
		writeSpaces(3);
		System.out.print(")");
		}
		public static void writeSpaces(int number) {
			for (int i =1; i <= number; i++){
				System.out.print(" ");
			}
	}
		// TODO Auto-generated method stub

	}


